(module
  (type $t (;0;) (func))
  (type (;1;) (func (param (ref $t))))
  (type (;2;) (func (param (ref func))))
  (type (;3;) (func (param (ref extern))))
  (func (;0;) (type 1) (param $r (ref $t))
    (drop
      (ref.is_null
        (local.get $r)))
  )
  (func (;1;) (type 2) (param $r (ref func))
    (drop
      (ref.is_null
        (local.get $r)))
  )
  (func (;2;) (type 3) (param $r (ref extern))
    (drop
      (ref.is_null
        (local.get $r)))
  )
)
